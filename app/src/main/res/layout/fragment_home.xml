        android:id="@+id/recyclerViewLoaiPhong"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:padding="8dp"
        tools:listitem="@layout/item_loai_phong" />

</RelativeLayout>
